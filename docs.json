{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Seminode",
  "colors": {
    "primary": "#8b5cf6",
    "light": "#a78bfa",
    "dark": "#7c3aed"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "https://api.seminode.com/v1/docs/public"
      }
    ]
  },
  "logo": {
    "light": "/logo/logo-long-black.png",
    "dark": "/logo/logo-long-white.png"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Create Company",
      "href": "https://app.seminode.com/auth/create-company"
    },
    "links": [
      {
        "label": "Log in",
        "href": "https://app.seminode.com/auth/login"
      }
    ]
  },
"footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/seminode"
    },
    "links": [
      {
        "header": "Company",
        "items": [
          {
            "label": "About",
            "href": "https://www.seminode.com/about-us"
          },
          {
            "label": "Log in",
            "href": "https://app.seminode.com/auth/login"
          },
          {
            "label": "Create Company",
            "href": "https://app.seminode.com/auth/create-company"
          }
        ]
      },
      {
        "header": "Legal",
        "items": [
          {
            "label": "Privacy Policy",
            "href": "https://www.seminode.com/legal/privacy-policy"
          },
          {
            "label": "Terms of Service",
            "href": "https://www.seminode.com/legal/service-terms"
          }
        ]
      }
    ]
  },
  "api": {
    "baseUrl": "https://api.seminode.com/v1",
    "auth": {
      "method": "bearer"
    }
  }
